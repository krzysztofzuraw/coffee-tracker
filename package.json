{
  "name": "coffee-tracker",
  "version": "0.1.0",
  "description": "Coffee brew tracker",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:tsc": "tsc --project . --noEmit",
    "lint:eslint": "eslint '*.{js,ts,tsx}' --quiet --fix",
    "reinstall": "rimraf node_modules && npm install",
    "test": "echo to be implemented!"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krzysztofzuraw/coffee-tracker.git"
  },
  "author": "Krzysztof Å»uraw <info@kzuraw.com> (https://krzysztofzuraw.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krzysztofzuraw/coffee-tracker/issues"
  },
  "homepage": "https://github.com/krzysztofzuraw/coffee-tracker#readme",
  "dependencies": {
    "@sentry/browser": "5.27.3",
    "@sentry/integrations": "5.27.3",
    "@sentry/node": "5.27.3",
    "idb": "5.0.7",
    "next": "9.5.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "6.9.6"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "node_modules",
      "dist",
      ".next"
    ],
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true
  },
  "devDependencies": {
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "concurrently": "5.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "pretty-quick": "3.0.2",
    "rimraf": "3.0.2",
    "typescript": "4.0.3"
  }
}
